#include <stdio.h>
#include <stdlib.h>

int main( int argc, char**argv){
	if(2>argc){
		printf("Nombre de param√®tres insuffisants\n");
		return 1;
	}
	//valeur negative
	if( atoi(argv[1]) < 0 )
		printf("%d n'est pas un entier naturel", atoi(argv[1]) );
	
	//debut prog
	int a = atoi(argv[1]);
	int b = atoi(argv[2]);
	int tmp = 0;

	a = a < 0 ? a*-1 : a;
	b = b < 0 ? b*-1 : b;
	do{
		tmp = a%b;
		a = b;
		b = tmp;
	}while( a != b && b != 0 );
	printf("%d", tmp);

return 0;
}
